#lang racket

(define(areatri b h)
  (/ (* b h)2))


(define(ex1 n)
  (+ 10 (expt n 2)))

(define(ex2 n)
  (+ (* (/ 1 2) (expt n 2)) 20))

(define(ex3 n)
  (- 2 (/ 1 n)))

(define(des a b c)
  (- (* b b) (* 4 (* a c))))

(define(cuad  a b c)
  (cond
    [(> (* b b) (* 4 (* a c))) 2]
    [(= (* b b) (* 4 (* a c))) 1]
    [(< (* b b) (* 4 (* a c))) 0]))